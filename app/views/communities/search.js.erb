(function() {
  const searchResultFirst = document.getElementById("search-result-first");
  if (searchResultFirst) {
  searchResultFirst.remove();
  }
  const searchResult = document.getElementById("search-result");
  searchResult.insertAdjacentHTML('afterbegin', `<%= render partial: 'search_community', locals: {search_communities: @search_communities} %>`);
  document.getElementById("search-input").value = "";
})();